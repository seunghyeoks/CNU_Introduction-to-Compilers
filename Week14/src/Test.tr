fn forl( ) -> i32 {
    let a = 0;
    for i in 1..10{
        a = a + i;
    }
    return a;
}

fn ll( ) -> i32 {
    let a = 0;
    loop {
        if a > 10 { break;}
        a = a + 1;
    }
    return a;
}

fn add(a:i32, b:i32) -> i32 {
    return a+b;
}

fn main() {
    let a = 1;
    let b = 2;
    let ret = 0;
    
    if (a > 1 && b > 1) {
        ret = forl( );
        println!("{}", ret);
    } else {
        ret = ll( );
        println!("{}", ret);
    }
    
    return;
}
